    "@types/supertest": "^6.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^9.0.0",
    "globals": "^16.4.0",
    "prettier": "^3.0.0",
    "supertest": "^7.0.0",
    "typescript": "^5.0.0",
